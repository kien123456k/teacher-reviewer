{
    "printWidth": 100,
    "tabWidth": 4,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "singleQuote": true,
    "semi": false,
    "endOfLine": "auto",
    "importOrder": [
        "^react",
        "(^@chakra-ui|components|containers)",
        "(^[src/]*$|^[./]|^[../])",
        "^[a-z\\-/]*$",
        ""
    ],
    "importOrderSeparation": true,
    "experimentalBabelParserPluginsList": ["js", "jsx"]
}
